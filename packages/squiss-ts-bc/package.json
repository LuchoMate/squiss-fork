{
  "name": "@squiss/squiss-ts-bc",
  "version": "1.0.0",
  "description": "High-volume SQS poller",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "^10 || ^12 || ^14 || ^15"
  },
  "scripts": {
    "compile": "tsc",
    "clean": "rm -rf dist .nyc_output *.log coverage",
    "build": "yarn lint && yarn compile",
    "build:fresh": "yarn clean && yarn build",
    "test": "yarn lint && yarn test:cover",
    "test:unit": "mocha --opts ../../mocha.opts",
    "test:cover": "nyc --reporter=lcov --reporter=text-summary yarn test:unit",
    "lint": "tslint -c ../../tslint.json 'src/**/*.ts' 'test/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PruvoNet/squiss-ts.git"
  },
  "files": [
    "/dist/*.d.ts",
    "/dist/*.js"
  ],
  "keywords": [
    "aws",
    "amazon",
    "sqs",
    "polling",
    "poll",
    "poller",
    "queue",
    "typescript",
    "sns",
    "s3",
    "extended",
    "client",
    "sdk"
  ],
  "author": "Regev Brody <regevbr@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/PruvoNet/squiss-ts/issues"
  },
  "homepage": "https://github.com/PruvoNet/squiss-ts#readme",
  "dependencies": {
    "@squiss/core": "^1.0.0",
    "aws-sdk": "^2.866.0",
    "ts-type-guards": "^0.7.0"
  },
  "devDependencies": {
  }
}
