{
  "private": true,
  "scripts": {
    "clean": "lerna run --parallel clean && rm -rf .nyc_output *.log coverage",
    "bootstrap": "lerna bootstrap",
    "compile": "lerna run --parallel compile",
    "build": "yarn lint && yarn compile",
    "build:fresh": "yarn clean && yarn build",
    "test": "yarn lint && yarn test:cover",
    "test:unit": "mocha --opts mocha.opts",
    "test:cover": "nyc --reporter=lcov --reporter=text-summary yarn test:unit",
    "lint": "tslint -c tslint.json 'packages/*/src/**/*.ts' 'packages/*/test/**/*.ts'"
  },
  "workspaces": [
    "packages/**"
  ],
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.8.1",
    "@aws-sdk/client-sqs": "^3.8.1",
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@types/chai": "^4.2.15",
    "@types/chai-as-promised": "^7.1.3",
    "@types/iltorb": "^2.3.2",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.35",
    "@types/proxyquire": "^1.3.28",
    "@types/semver": "^7.3.4",
    "@types/uuid": "^8.3.0",
    "aws-sdk": "^2.864.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "delay": "^4.4.1",
    "dirty-chai": "^2.0.1",
    "lerna": "^4.0.0",
    "mocha": "6.2.2",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.5.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  }
}
